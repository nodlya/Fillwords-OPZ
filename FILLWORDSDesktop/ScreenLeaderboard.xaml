        <ListBox x:Name="PlayersList" Height="300" Width="300" 
                 Canvas.Left="258" Canvas.Top="60" 
                 Background="#FFF6E9BA" BorderThickness="0">
            
        </ListBox>

        <Button Content="Меню" Height="30" Width="50" Background="AliceBlue" 
                Canvas.Left="10" Canvas.Top="10" MouseDown="BackToMenu"/>
    </Canvas>
</Window>
